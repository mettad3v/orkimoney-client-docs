                              },
                              "exchange_rate": { "type": "number", "example": 93023.26 },
                              "is_best": { "type": "boolean", "example": true },
                              "fiat_amount": { "type": "number", "example": 350 },
                              "crypto_amount": { "type": "number", "example": 0.0037606 },
                              "quote_amount": { "type": "number", "example": 0.0037606 },
                              "link": { "type": "string", "nullable": true, "example": "buy-link" }
                            }
                          }
                        }
                      }
                    },
                    "success": { "type": "boolean", "example": true },
                    "code": { "type": "integer", "example": 200 }
